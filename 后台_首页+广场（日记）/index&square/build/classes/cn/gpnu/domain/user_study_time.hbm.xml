<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.gpnu.domain">

	<class name="user_study_time" table="user_study_time">
		<id name="study_time_id">
			<generator class="native"></generator>
		</id>

		<property name="openid" column="openid" length="28"></property>
		<property name="dayly_time" column="dayly_time"></property>
		<property name="weekly_time" column="weekly_time"></property>
		<property name="monthly_time" column="monthly_time"></property>
		<property name="updated_at" column="updated_at"></property>
		<property name="created_at" column="created_at"></property>

	</class>
</hibernate-mapping>